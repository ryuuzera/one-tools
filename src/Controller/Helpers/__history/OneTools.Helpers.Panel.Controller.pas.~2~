unit OneTools.Helpers.Panel.Controller;

interface

uses
  Vcl.ExtCtrls, OneTools.Styles.Constants.View, System.Classes;

type
  TOnePanel = class(TCustomPanel)

  private

  public

  published
    property OnMouseEnter;
    property OnMouseLeave;
    property Color default BUTTON_COLOR;

  end;

  procedure Register;

implementation

procedure Register;
begin
  RegisterComponents('FMS Components', [TOnePanel]);
end;

end.
